{
  "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroupName": {
      "type": "string",
      "defaultValue": "cloud-shell-storage-southeastasia",
      "metadata": {
        "_parameterType": "resourceGroup",
        "description": "리소스에 대한 리소스 그룹의 이름입니다. 추적 기능 향상을 위해 리소스를 같은 리소스 그룹에 배치하는 것이 좋습니다."
      }
    },
    "resourceGroupLocation": {
      "type": "string",
      "defaultValue": "southeastasia",
      "metadata": {
        "_parameterType": "location",
        "description": "리소스 그룹의 위치입니다. 리소스 그룹의 위치는 리소스와 다를 수 있습니다."
      }
    },
    "resourceLocation": {
      "type": "string",
      "defaultValue": "[parameters('resourceGroupLocation')]",
      "metadata": {
        "_parameterType": "location",
        "description": "리소스의 위치입니다. 리소스 공급자가 해당 위치에서 지원되지 않는 경우를 제외하고는 기본적으로 리소스 그룹의 위치를 사용합니다."
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/resourceGroups",
      "name": "[parameters('resourceGroupName')]",
      "location": "[parameters('resourceGroupLocation')]",
      "apiVersion": "2019-10-01"
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "[concat(parameters('resourceGroupName'), 'Deployment', uniqueString(concat('cs110030000a95bff29', subscription().subscriptionId)))]",
      "resourceGroup": "[parameters('resourceGroupName')]",
      "apiVersion": "2019-10-01",
      "dependsOn": [
        "[parameters('resourceGroupName')]"
      ],
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "resources": [
            {
              "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
              },
              "kind": "StorageV2",
              "name": "cs110030000a95bff29",
              "type": "Microsoft.Storage/storageAccounts",
              "location": "[parameters('resourceLocation')]",
              "apiVersion": "2017-10-01"
            }
          ]
        }
      }
    }
  ],
  "metadata": {
    "_dependencyType": "storage.azure"
  }
}